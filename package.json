{
  "name": "annotator-content-analysis",
  "version": "1.0.0",
  "description": "A content analysis plugin for annotator",
  "repository": {
    "type": "git",
    "url": "git@github.com:Goodly/TextThresher.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=dev node --harmony ./webpack/dev-server.js",
    "serve": "NODE_ENV=development DEBUG=dev webpack-dev-server --config webpack.config.js",
    "build": "NODE_ENV=development DEBUG=dev webpack --progress --stats --config webpack.config.js",
    "build:quiet": "NODE_ENV=development DEBUG=dev webpack --stats --config webpack.config.js",
    "build:watch": "NODE_ENV=development DEBUG=dev webpack --progress --stats --config webpack.config.js --watch",
    "deploy": "NODE_ENV=production webpack --progress --stats --config webpack.config.js",
    "deploy:quiet": "NODE_ENV=production webpack --stats --config webpack.config.js",
    "test": "node node_modules/.bin/jest",
    "test:watch": "npm run test -- --watch",
    "tasks:highlighter": "curl http://localhost:5000/api/highlighter_tasks/?format=json | python -m json.tool > pbs-highlighter/highlighter_tasks.json",
    "tasks:quiz": "curl http://localhost:5000/api/quiz_tasks/?format=json | python -m json.tool > pbs-quiz/quiz_tasks.json"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.16.3",
    "babel-require": "^1.0.1",
    "babel-runtime": "^6.23.0",
    "caniuse-db": "^1.0.30000624",
    "clean-webpack-plugin": "^0.1.16",
    "color": "^1.0.3",
    "colors": "^1.1.2",
    "css-loader": "^0.26.0",
    "csswring": "^5.1.0",
    "debug": "^2.1.3",
    "del": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-module-utils": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "estraverse-fb": "^1.3.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-ponyfill": "^4.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.3",
    "intro.js": "^2.5.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.1",
    "moment": "^2.15.2",
    "node-sass": "^4.4.0",
    "normalizr": "^2.3.1",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^1.1.1",
    "radium": "^0.18.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-shallow-compare": "^15.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dates": "^4.0.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-portal": "^3.0.0",
    "react-redux": "^4.2.1",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.2.1",
    "redux-devtools": "^3.1.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.12.2",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "modulePaths": [
      "<rootDir>/app",
      "<rootDir>/vendor/bower_components"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "web_modules"
    ],
    "moduleNameMapper": {
      "^modernizr$": "<rootDir>/vendor/bower_components/modernizr/modernizr.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
